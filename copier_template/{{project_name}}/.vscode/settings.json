{
    // black set line length to 120
    "black-formatter.args": [
        "--line-length",
        "120"
    ],
    // Here, you can integrate your job configs
    "json.schemas": [
        {
            "fileMatch": [
                "job_configs/quickstart.jsonc" // replace "quickstart" with your experiment's name
            ],
            "url": ".vscode/quickstart.schema.json" // replace "quickstart" with your experiment's name
        }
    ]
}